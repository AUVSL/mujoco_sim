<launch>
    <arg name="enable_ekf" default="false"/>

    <include file="$(find mujoco_sim)/launch/mujoco_sim.launch">
        <arg name="robot" value="$(find mujoco_sim)/model/warthog/warthog_robot.xml" />
        <arg name="world" value="$(find mujoco_sim)/model/warthog/warthog_world.xml" />
    </include>

    <include file="$(find warthog_description)/launch/description.launch" />

    <include file="$(find warthog_control)/launch/control.launch">
        <arg name="enable_ekf" value="$(arg enable_ekf)" />
    </include>

</launch>
