    <!-- https://ease-crc.org/ont/USD#CollisionAPI -->

    <owl:Class rdf:about="https://ease-crc.org/ont/USD#CollisionAPI">
        <rdfs:subClassOf rdf:resource="https://ease-crc.org/ont/USD#APISchema"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#describes"/>
                <owl:allValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://ease-crc.org/ont/USD#hasPhysics"/>
