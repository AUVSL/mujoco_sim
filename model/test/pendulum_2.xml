<mujoco>
    <asset>
        <texture name="grid" type="2d" builtin="checker" width="512" height="512" rgb1=".1 .2 .3"
            rgb2=".2 .3 .4" />
        <material name="grid" texture="grid" texrepeat="1 1" texuniform="true" reflectance=".2" />
    </asset>

    <worldbody>
        <geom name="floor" size="0 0 .05" type="plane" material="grid" />
        <light diffuse=".5 .5 .5" pos="0 0 5" dir="0 0 -1" />
    </worldbody>

    <worldbody>
        <body name="cube_2" pos="-0.5 0.866 2">
            <joint type="ball" pos="0.5 -0.866 0" />
            <geom type="box" size=".1 .1 .1" rgba="0 .9 0 1" />
        </body>
        <body name="cube_1_ref" pos="1 0 2" mocap="true">
            <geom type="box" size=".1 .1 .1" rgba="0.5 0.5 0.5 1" />
        </body>
        <body name="cube_1" pos="1 0 2">
            <freejoint />
            <geom type="box" size=".1 .1 .1" rgba=".9 0 0 1" />
        </body>
        <body name="cube_3_ref" pos="-0.5 -0.866 2" mocap="true">
            <geom type="box" size=".1 .1 .1" rgba="0.5 0.5 0.5 1" />
        </body>
        <body name="cube_3" pos="-0.5 -0.866 2">
            <freejoint />
            <geom type="box" size=".1 .1 .1" rgba=".9 0 0 1" />
        </body>
    </worldbody>

    <equality>
        <weld body1="cube_1" body2="cube_1_ref" torquescale="0.9" />
        <weld body1="cube_3" body2="cube_3_ref" torquescale="0.9" />
    </equality>

    <contact>
        <exclude body1="cube_1" body2="cube_1_ref" />
        <exclude body1="cube_2" body2="cube_1_ref" />
        <exclude body1="cube_3" body2="cube_1_ref" />
        <exclude body1="cube_1" body2="cube_3_ref" />
        <exclude body1="cube_2" body2="cube_3_ref" />
        <exclude body1="cube_3" body2="cube_3_ref" />
        <exclude body1="cube_1_ref" body2="cube_3_ref" />
    </contact>
</mujoco>